{
  "extends": "eslint:recommended",
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "no-console": 0,
    "space-after-keywords": 2
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "globals": {}
}
